<?xml version='1.0' encoding='utf-8'?>
<widget id="org.cordova.quasar.app" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Quasar App</name>
    <description>A Quasar Framework app</description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <splash src="../app-resources/ios/splash/Default@2x~universal~anyany.png" />
        <icon height="40" src="../app-resources/ios/icons/icon-40.png" width="40" />
        <icon height="80" src="../app-resources/ios/icons/icon-40@2x.png" width="80" />
        <icon height="50" src="../app-resources/ios/icons/icon-50.png" width="50" />
        <icon height="100" src="../app-resources/ios/icons/icon-50@2x.png" width="100" />
        <icon height="120" src="../app-resources/ios/icons/icon-60@2x.png" width="120" />
        <icon height="180" src="../app-resources/ios/icons/icon-60@3x.png" width="180" />
        <icon height="72" src="../app-resources/ios/icons/icon-72.png" width="72" />
        <icon height="144" src="../app-resources/ios/icons/icon-72@2x.png" width="144" />
        <icon height="76" src="../app-resources/ios/icons/icon-76.png" width="76" />
        <icon height="152" src="../app-resources/ios/icons/icon-76@2x.png" width="152" />
        <icon height="167" src="../app-resources/ios/icons/icon-83.5@2x.png" width="167" />
        <icon height="29" src="../app-resources/ios/icons/icon-small.png" width="29" />
        <icon height="58" src="../app-resources/ios/icons/icon-small@2x.png" width="58" />
        <icon height="87" src="../app-resources/ios/icons/icon-small@3x.png" width="87" />
        <icon height="57" src="../app-resources/ios/icons/icon.png" width="57" />
        <icon height="114" src="../app-resources/ios/icons/icon@2x.png" width="144" />
    </platform>
    <allow-navigation href="about:*" />
</widget>
